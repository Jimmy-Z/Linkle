{
	"manifest_version": 2,
	"name": "Linkle",
	"version": "0.1.3",
	"description": "Send link to aria2 RPC",
	"icons": { "64": "icon.png" },
	"background": { "scripts": ["linkle.js"], "persistent": false},
	"options_ui": { "page": "options.html" },
	"permissions": [ "contextMenus", "notifications", "storage", "activeTab"],
	"optional_permissions": ["cookies", "http://*/", "https://*/"],
	"short_name": "Linkle"
}
